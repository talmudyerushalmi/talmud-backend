{
  "name": "talmud-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "runconsole": "ts-node -r tsconfig-paths/register src/console.ts normalize:excerpts",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start-win": "set NODE_ENV=dev&& nest start --watch ",
    "start:dev-prod": "NODE_ENV=production nest start --watch",
    "start-win-dev-prod": "set NODE_ENV=production&& nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test-start:e2e": "dotenv-run-script .env test:e2e",
    "test:e2e": "jest --config jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-express": "^10.2.10",
    "@types/mongodb": "^4.0.7",
    "@types/string-similarity": "^4.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "colors": "^1.4.0",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "diff": "^5.1.0",
    "draft-js": "^0.11.7",
    "express-jwt": "^8.4.1",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "nest-csv-parser": "^2.0.4",
    "nestjs-console": "^9.0.0",
    "npm": "^10.2.5",
    "numeral": "^2.0.6",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "string-similarity": "^4.0.4",
    "xmlbuilder": "^15.1.1",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/draft-js": "^0.11.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "dotenv": "^16.3.1",
    "dotenv-run-script": "^0.4.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
